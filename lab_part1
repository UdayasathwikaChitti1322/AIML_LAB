        "import numpy as np\n",
        "list=[\"Sathvika\",\"leena\",\"Greeshma\"]\n",
        "print(\"List=\",list)\n",
        "arr=np.array([1,2,3,4,5])\n",
        "print(\"Array=\",arr)\n",
        "print(\"arr[0]=\",arr[0])\n",
        "print(\"arr[1]=\",arr[1])\n",
        "print(\"arr[2]=\",arr[2])\n",
        "print(\"arr[3]=\",arr[3])\n",
        "print(\"arr[4]=\",arr[4])\n",
        "print(type(list))\n",
        "print(type(arr))\n",
        "dataset1=np.array([1,2,3])\n",
        "dataset2=np.array([4,5,6])\n",
