        "    return math.sin(x)\n",
        "def log(x, base=math.e):\n",
        "    return math.log(x,base)\n",
        "def log2(x):\n",
        "    return math.log2(x)\n",
        "def exp(x):\n",
        "    return math.exp(x)\n",
